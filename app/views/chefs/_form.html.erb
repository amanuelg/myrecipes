<%= render 'shared/error_messages', obj: @chef %>
<p>
<div class="row">
        <div class="col-md-8 col-md-offset-2 well">
            <%= form_for @chef do |f| %>
            
            <%= f.label :chefname%>
            <%= f.text_field :chef_name %>
            
            <%= f.label :email %>
            <%= f.text_field :email%>
            
            <%= f.label :password %>
            <%= f.password_field :password %>
            
            <%= f.submit @chef.new_record? ? 'Register' : 'Edit Profile', class: 'btn btn-success'%>
            <% end %>
            
            
        </div>
    </div>
    
</p>


